{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "FHI360DEMO2"
		},
		"ls_AzureDataLakeStorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_AzureDataLakeStorage'"
		},
		"ls_AzureDataLakeStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://fhi360storage.dfs.core.windows.net/"
		},
		"ls_AzureKVDemo_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://demokeyvaultfhi360.vault.azure.net/"
		},
		"ls_ProfiseeRESTAPI_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sbprofisee.onebridge.xyz/profisee/rest/v1"
		},
		"trigger2_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/6eeab5e8-d068-4132-9d68-67a504a3069c/resourceGroups/FHI360-Demo/providers/Microsoft.Storage/storageAccounts/fhi360storage"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "FHI360DEMO2_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_AzureDataLakeStorage_accountKey": {
						"value": "[parameters('ls_AzureDataLakeStorage_accountKey')]"
					},
					"ls_AzureDataLakeStorage_properties_typeProperties_url": {
						"value": "[parameters('ls_AzureDataLakeStorage_properties_typeProperties_url')]"
					},
					"ls_AzureKVDemo_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_AzureKVDemo_properties_typeProperties_baseUrl')]"
					},
					"ls_ProfiseeRESTAPI_properties_typeProperties_url": {
						"value": "[parameters('ls_ProfiseeRESTAPI_properties_typeProperties_url')]"
					},
					"trigger2_properties_typeProperties_scope": {
						"value": "[parameters('trigger2_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}